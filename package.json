{
  "name": "rahulhostel",
  "version": "1.0.0",
  "description": "Rahul hostel project file package",
  "main": "index.js",
  "scripts": {
    "compile:sass__timeout__5": "for /l %g in () do @( node-sass src/sass/main.scss src/css/style.css & timeout /t 5 )",
    "compile:sass": "node-sass src/sass/main.scss src/css/style.css",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver compile:sass__timeout__5",
    "compile:css": "node-sass src/sass/main.scss dist/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" dist/css/style.comp.css -o dist/css/style.prefix.css",
    "compress:css": "node-sass dist/css/style.prefix.css dist/css/style.css --output-style compressed",
    "build": "npm-run-all compile:css prefix:css compress:css",
    "webpack-dev": "webpack --mode development",
    "webpack-build": "webpack --mode production",
    "webpack-start": "webpack-dev-server --mode development --open --port 3000"
  },
  "author": "Joseph",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "autoprefixer": "^9.8.5",
    "babel-loader": "^8.1.0",
    "concat": "^1.0.3",
    "core-js": "^3.6.5",
    "html-webpack-plugin": "^4.3.0",
    "install": "^0.13.0",
    "npm": "^6.14.7",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "regenerator-runtime": "^0.13.7",
    "sass": "^1.26.10",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.10.4",
    "ol": "^6.4.3",
    "source-map-loader": "^1.1.0"
  },
  "engines": {
    "node": "^10"
  }
}
